  default_value: ''
  description: Effective duration of an exposure, corrected for dead time (i.e., it
    is the photon counting time); equivalent to keyword EFFEXPTM (seconds).
  destination:
  - ScienceCommonExt.xposure
  example: '21.474'
  fits_hdu: SCI
  fits_keyword: XPOSURE
  level: 1b
  misc: Value is CompletedExposure.photonCollectingTime, but set from EFFEXPTM. Added
    by JWSTKD-91 to align with the FITS standard for integration times.
  mode: All
  section: Exposure
  si: Multiple
  source: Science Data Processing (SDP)
  special_processing: VALUE_REQUIRED
  sql_dtype: float
  sw_source: ''
  title: '[s] Effective exposure time'
  type: float
  units: seconds
elapsed_exposure_time:
  calculation: input('MJD-END') - ('MJD-BEG')
  default_value: ''
  description: Total duration of the exposure, including dead times; computed as the
    difference between exposure end and start times; equivalent to keyword DURATION
    (seconds).
  destination:
  - ScienceCommonExt.telapse
  example: '3999.632'
  fits_hdu: SCI
  fits_keyword: TELAPSE
  level: 1b
  misc: added by JWSTKD-92 to align with the FITS standard for effective integration
    times
  mode: All
  section: Exposure
  si: ALL
  source: Science Data Processing (SDP)
  special_processing: VALUE_REQUIRED
  sql_dtype: float
  sw_source: ''
  title: '[s] Total elapsed exposure time'
  type: float
  units: seconds
