mt_avdec:
  calculation: ''
  default_value: ''
  description: MT_AVDEC gives the declination of a moving target averaged over all
    exposures within an observation. It is the simple mean of the MT_DEC values in
    each exposure. The average position is used to compute an appropriate WCS offset
    for each exposure so that the target is properly aligned when creating combined
    images/spectra.
  destination:
  - ScienceCommonExt.mt_avdec
  example: '-42.315892'
  fits_hdu: SCI
  fits_keyword: MT_AVDEC
  level: 2c
  misc: ''
  mode: All
  section: WCS Parameters
  si: ALL
  source: Calibration Software
  special_processing: OPTIONAL
  sql_dtype: float
  sw_source: ''
  title: '[deg] Moving target average DEC over exposures'
  type: float
  units: deg
mt_avra:
  calculation: ''
  default_value: ''
  description: MT_AVRA gives the right ascension of a moving target averaged over
    all exposures within an observation. It is the simple mean of the MT_RA values
    in each exposure. The average position is used to compute an appropriate WCS offset
    for each exposure so that the target is properly aligned when creating combined
    images/spectra.
  destination:
  - ScienceCommonExt.mt_avra
  example: '42.315892'
  fits_hdu: SCI
  fits_keyword: MT_AVRA
  level: 2c
  misc: ''
  mode: All
  section: WCS Parameters
  si: ALL
  source: Calibration Software
  special_processing: OPTIONAL
  sql_dtype: float
  sw_source: ''
  title: '[deg] Moving target average RA over exposures'
  type: float
  units: deg
